{{>layout/header}}
<h1>판매자 상품 리스트</h1>
<div class="col-md-12">
  <div class="row">
    <div class="col-md-6">
      <a href="/post/savePost" role="button" class="btn-btn-primary">글 등록</a>
    </div>
  </div>
  <br>
  <table class="table table-horizontal table-boarded">
    <thead class="thead-strong">
    <tr>
      <th>Post 번호</th>
      <th>제목</th>
      <th>판매자</th>
      <th>내용</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#postList}}
      <tr>
        <td>{{id}}</td>
        <td>{{title}}</td>
        <td>{{seller}}</td>
        <td>{{content}}</td>
      </tr>
    {{/postList}}
    </tbody>
  </table>
</div>
{{>layout/footer}}