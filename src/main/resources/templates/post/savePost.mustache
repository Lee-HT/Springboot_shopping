{{>layout/header}}
<h1>판매글 등록</h1>
<div class="col-md-12">
  <div class="col-md-4">
    <form action="/postProcess/savePost" method="post">
      <div class="form-group">
        <label>판매자</label>
        <input type="text" class="form-control" id="seller" placeholder="판매자를 입력하세요">
      </div>
      <div class="form-group">
        <label>제목</label>
        <input type="text" class="form-control" id="title" placeholder="제목을 입력하세요">
      </div>
      <div class="form-group">
        <label>내용</label>
        <textarea class="form-control" rows="4" id="content" placeholder="내용을 입력하세요"></textarea>
      </div>
      <button type="submit" class="btn btn-primary" id="btn-save">등록</button>
    </form>
    <a href ="/" role="button" class="btn btn-secondary">취소</a>
  </div>

</div>
{{>layout/footer}}